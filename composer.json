{
  "name": "fmarquesto/sap-business-one-connector",
  "description": "Utility to connect with SAP Business One through the Service Layer API",
  "type": "library",
  "keywords": [
    "sap",
    "sapb1",
    "business-one",
    "connector",
    "api",
    "integration",
    "service-layer"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Fede Marquesto",
      "email": "fede.marquesto@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.5",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.39",
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "fmarquesto\\SapBusinessOneConnector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "fix": "php-cs-fixer fix src"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
